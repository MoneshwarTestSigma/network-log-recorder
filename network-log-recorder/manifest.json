{
    "manifest_version": 3,
    "name": "Network Call Tracker",
    "version": "1.1",
    "description": "Capture and log network calls without opening DevTools.",
    "permissions": [
      "webRequest",
      "activeTab",
      "scripting",
      "storage",
      "downloads"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  